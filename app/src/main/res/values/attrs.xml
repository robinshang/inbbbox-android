<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AspectRatioImageView">
        <attr name="aspectRatio" format="float"/>
        <attr name="dominantMeasurement">
            <enum name="width" value="0"/>
            <enum name="height" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="RoundedCornersView">
        <attr name="roundedCornersBackgroundColor" format="color|reference"/>
        <attr name="roundingBottomCornersEnabled" format="boolean|reference"/>
        <attr name="roundingEnabled" format="boolean|reference"/>
    </declare-styleable>
    <declare-styleable name="ScrollingBackgroundView">
        <attr name="scrollDirection">
            <flag name="up" value="-1"/>
            <flag name="down" value="1"/>
        </attr>
        <attr name="imageDrawable" format="reference"/>
        <attr name="offset" format="integer"/>
    </declare-styleable>
</resources>